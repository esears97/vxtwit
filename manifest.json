{
    "manifest_version": 2,
    "name": "VxTwitter",
    "version": "1.0",
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": [
          "https://twitter.com/*"
        ],
        "js": [
          "js/content.js"
        ]
      }
    ],
    "web_accessible_resources": [
      "images/VXBtnIcon.png"
    ],
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
  }
  
